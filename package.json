{
  "name": "js-proxy",
  "version": "0.1.0",
  "license": "MIT",
  "packageManager": "yarn@4.4.0",
  "author": {
    "name": "Joris Aerts",
    "url": "https://github.com/JorisAerts"
  },
  "files": [
    "dist/**/*",
    "cert/**/*"
  ],
  "scripts": {
    "build:node": "tsc -p src/node --outDir dist --esModuleInterop | true && tsc-alias --dir dist",
    "run:node": "node dist/node/index.js",
    "build": "yarn build:node && yarn build:client",
    "dev": "vite",
    "build:client": "vite build",
    "preview": "vite preview"
  },
  "private": true,
  "main": "dist/index.js",
  "type": "module",
  "dependencies": {
    "node-forge": "^1.3.1",
    "picocolors": "^1.0.1",
    "uuid": "^10.0.0",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@eslint/js": "9.8.0",
    "@types/node": "22.1.0",
    "@types/node-forge": "^1.3.11",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.5.12",
    "@vitejs/plugin-vue": "5.1.2",
    "@vitejs/plugin-vue-jsx": "4.0.0",
    "eslint": "9.8.0",
    "eslint-plugin-vue": "9.27.0",
    "globals": "15.9.0",
    "pinia": "2.2.1",
    "postcss": "^8.4.41",
    "prettier": "3.3.3",
    "sass": "1.77.8",
    "stylelint": "^16.8.1",
    "stylelint-config-standard-scss": "^13.1.0",
    "tsc-alias": "^1.8.10",
    "type-fest": "^4.23.0",
    "typescript": "^5.5.4",
    "typescript-eslint": "8.0.1",
    "vite": "5.4.0",
    "vite-plugin-vue-devtools": "7.3.7",
    "vue": "3.4.37",
    "vue-router": "4.4.3"
  },
  "resolutions": {
    "postcss": "^8.4.39"
  }
}
