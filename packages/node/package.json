{
  "name": "@jorisaerts/js-proxy",
  "version": "0.1.0",
  "description": "A reverse proxy server with a GUI",
  "keywords": [
    "reverse proxy",
    "proxy",
    "server",
    "web",
    "node",
    "ssl",
    "http",
    "https",
    "monitoring"
  ],
  "license": "MIT",
  "packageManager": "yarn@4.4.0",
  "main": "dist/index.js",
  "bin": "dist/index.js",
  "type": "module",
  "author": {
    "name": "Joris Aerts",
    "url": "https://github.com/JorisAerts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/JorisAerts/js-proxy.git"
  },
  "files": [
    "dist/**/*",
    "cert/**/*"
  ],
  "exports": {
    ".": "./dist/index.js"
  },
  "scripts": {
    "build:client": "cd ../client && yarn build && cp -R ./dist ../node/dist/client",
    "build": "vite build",
    "build:all": "yarn build && yarn build:client",
    "serve": "node dist/index.js",
    "test": "vitest --typecheck"
  },
  "dependencies": {
    "@mongodb-js/zstd": "1.2.0",
    "js-proxy-shared": "workspace:*",
    "node-forge": "1.3.1",
    "picocolors": "1.0.1",
    "uuid": "10.0.0",
    "ws": "8.18.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.0",
    "@types/node": "^22.5.0",
    "@types/node-forge": "^1.3.11",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.5.12",
    "eslint": "^9.9.0",
    "globals": "^15.9.0",
    "prettier": "^3.3.3",
    "type-fest": "^4.25.0",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.2.0",
    "vite": "^5.4.2",
    "vitest": "^2.0.5"
  }
}
