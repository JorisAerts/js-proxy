{
  "name": "lopr-ui",
  "version": "0.1.0",
  "description": "A reverse proxy server with a GUI",
  "keywords": [
    "reverse proxy",
    "proxy",
    "server",
    "web",
    "node",
    "ssl",
    "http",
    "https",
    "monitoring"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "type": "module",
  "author": {
    "name": "Joris Aerts",
    "url": "https://github.com/JorisAerts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/JorisAerts/lopr.git"
  },
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "check:type": "tsc --noEmit",
    "build": "vite build",
    "test": "vitest --typecheck"
  },
  "exports": {
    ".": "./src/index.ts",
    "./components": "./src/components/index.ts",
    "./components/*": "./src/components/*/index.ts",
    "./icons": "./src/icons/index.ts",
    "./icons/*": "./src/icons/*.ts",
    "./style/main.scss": "./src/style/main.scss",
    "./style": "./src/style/index.ts",
    "./style/*": {
      "sass": "./src/style/*.scss"
    },
    "./utils": "./src/utils/index.ts",
    "./utils/*": "./src/utils/*.ts",
    "./*": "./src/*"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@types/node": "^22.5.2",
    "@vitejs/plugin-vue": "^5.1.3",
    "@vitejs/plugin-vue-jsx": "^4.0.1",
    "eslint": "^9.9.1",
    "eslint-plugin-vue": "^9.27.0",
    "globals": "^15.9.0",
    "postcss": "^8.4.43",
    "prettier": "^3.3.3",
    "sass": "^1.77.8",
    "stylelint": "^16.9.0",
    "stylelint-config-standard-scss": "^13.1.0",
    "type-fest": "^4.26.0",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2",
    "vite-plugin-vue-devtools": "^7.3.9",
    "vitest": "^2.0.5",
    "vue": "^3.4.38"
  },
  "peerDependencies": {
    "vue": ">= 3.4 < 4"
  },
  "resolutions": {
    "postcss": "^8.4.41"
  }
}
